{
  "name": "contracts",
  "root": "apps/contracts",
  "sourceRoot": "apps/contracts",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "anchor build",
        "cwd": "apps/contracts"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "anchor test",
        "cwd": "apps/contracts"
      }
    },
    "run:test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "anchor run test-staking",
        "cwd": "apps/contracts"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "anchor deploy",
        "cwd": "apps/contracts"
      }
    }
  }
}
